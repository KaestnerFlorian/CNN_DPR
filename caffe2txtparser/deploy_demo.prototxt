    local_size: 5
    alpha: 0.0001
    beta: 0.75
  }
}
layer {
  name: "conv3_face"
  type: "Convolution"
  bottom: "norm2_face"
  top: "conv3_face"
  param {
    lr_mult: 0.2
    decay_mult: 1
  }
  param {
    lr_mult: 0.4
    decay_mult: 0
  }
  convolution_param {
    num_output: 384
    pad: 1
    kernel_size: 3
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu3_face"
  type: "ReLU"
  bottom: "conv3_face"
  top: "conv3_face"
}
layer {
  name: "conv4_face"
  type: "Convolution"
  bottom: "conv3_face"
  top: "conv4_face"
  param {
    lr_mult: 0.2
    decay_mult: 1
  }
  param {
    lr_mult: 0.4
    decay_mult: 0
  }
  convolution_param {
    num_output: 384
    pad: 1
    kernel_size: 3
    group: 2
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0.1
    }
  }
}
layer {
  name: "relu4_face"
  type: "ReLU"
  bottom: "conv4_face"
  top: "conv4_face"
}
layer {
  name: "conv5_face"
  type: "Convolution"
  bottom: "conv4_face"
  top: "conv5_face"
  param {
    lr_mult: 0.2
    decay_mult: 1
  }
  param {
    lr_mult: 0.4
    decay_mult: 0
  }
  convolution_param {
    num_output: 256
    pad: 1
    kernel_size: 3
    group: 2
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
