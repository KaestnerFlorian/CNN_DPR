  top: "conv5_face"
}
layer {
  name: "pool5_face"
  type: "Pooling"
  bottom: "conv5_face"
  top: "pool5_face"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
layer {
  name: "fc6_face"
  type: "InnerProduct"
  bottom: "pool5_face"
  top: "fc6_face"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 500
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0.5
    }
  }
}
layer {
  name: "relu6_face"
  type: "ReLU"
  bottom: "fc6_face"
  top: "fc6_face"
}
layer {
  name: "eyes_grid_flat"
  type: "Flatten"
  bottom: "eyes_grid"
  top: "eyes_grid_flat"
}
layer {
  name: "eyes_grid_mult"
  type: "Power"
  bottom: "eyes_grid_flat"
  top: "eyes_grid_mult"
  power_param{
	power: 1
	scale: 24
	shift: 0
  }
}

layer {
  name: "face_input"
  type: "Concat"
  bottom: "fc6_face"
  bottom: "eyes_grid_mult"
  top: "face_input"
  concat_param {
    axis: 1
  }
}
layer {
  name: "fc7_face"
  type: "InnerProduct"
  bottom: "face_input"
  top: "fc7_face"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 400
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0.5
    }
  }
}
layer {
  name: "relu7_face"
  type: "ReLU"
  bottom: "fc7_face"
  top: "fc7_face"
}
layer {
  name: "fc8_face"
  type: "InnerProduct"
  bottom: "fc7_face"
  top: "fc8_face"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 200
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0.5
    }
  }
}
layer {
  name: "relu8_face"
  type: "ReLU"
  bottom: "fc8_face"
  top: "fc8_face"
}
layer {
  name: "importance_no_sigmoid"
  type: "InnerProduct"
  bottom: "fc8_face"
  top: "importance_no_sigmoid"
  param {
    lr_mult: 0.2
    decay_mult: 1
  }
  inner_product_param {
    num_output: 169
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
  }
}

layer {
  name: "importance_map_prefilter"
  type: "Sigmoid"
  bottom: "importance_no_sigmoid"
  top: "importance_map_prefilter"
}

layer {
  name: "importance_map_reshape"
  type:"Reshape"
  bottom: "importance_map_prefilter"
  top: "importance_map_reshape"
  reshape_param {
      shape {
      dim: 1
      dim: 1
      dim: 13
      dim: 13
      }
  }
}
layer {
  name: "importance_map"
  type: "Convolution"
  bottom: "importance_map_reshape"
  top: "importance_map"
  param {
    lr_mult: 0
    decay_mult: 1
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  convolution_param {
    num_output: 1
    pad: 1
    kernel_size: 3
    stride: 1
  }
}

layer {
  name: "fc_7"
  type: "Eltwise"
  bottom: "conv5_red"
  bottom: "importance_map"
  top: "fc_7"
  eltwise_param {
    operation: PROD
  }
}
layer {
  name: "fc_0_0"
  type: "InnerProduct"
  bottom: "fc_7"
  top: "fc_0_0"
